datasource db {
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
}

model Merchant {
  id            String   @id
  name          String
  location      String
  lat          Float?   
  lng          Float?   
  registeredAt  DateTime @default(now())
  score         Int      @default(0)
  scoreBreakdown Json
  stats         Json
}